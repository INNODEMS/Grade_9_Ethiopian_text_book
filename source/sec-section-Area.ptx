              \node at (4,5){(a)};
              \node at (4.5,2){4m};
              \node at (2,-0.7){5m};
              \draw [dashed,fill=black] (4,4) --(4,0);
              \draw(3.5,0) coordinate (A)-- (3.5,0.5) coordinate (B)-- (4,0.5) coordinate (C)--  (4,0) coordinate(D)-- (3.5,0) coordinate (A);
            \end{tikzpicture}
          </latex-image>
            
          </image>
          <image width="40%">
            <latex-image>
              \begin{tikzpicture}
                \draw [thick,fill=white](0,0) coordinate (A)-- (4,4) coordinate (B)-- (8,4) coordinate (C)--(4,0) coordinate (D)-- (0,0) coordinate (A);
                \node at (4,5){(b)};
                \node at (6,4.3){9cm};
                \node at (4.5,2){7cm};
                \draw [dashed,fill=black] (4,0) --(4,4);
                \draw(4,3.5) coordinate (A)-- (4,4) coordinate (B)-- (4.5,4) coordinate (C)--  (4.5,3.5) coordinate(D)-- (4,3.5) coordinate (A);
              \end{tikzpicture}
